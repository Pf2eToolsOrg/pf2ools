{
	"useTabs": true,
	"singleQuote": false,
	"printWidth": 120,
	"trailingComma": "es5",
	"tabWidth": 4,
	"semi": true,
	"bracketSpacing": true,
	"arrowParens": "always",
	"svelteAllowShorthand": true,
	"svelteIndentScriptAndStyle": true,
	"svelteSortOrder": "options-scripts-markup-styles",
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
